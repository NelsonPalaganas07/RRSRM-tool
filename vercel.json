{
    "version": 2,
    "builds": [
      {
        "src": "api/backend/app.py",
        "use": "@vercel/python",
        "config": { "memory": 2048 }
      },
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/backend/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      }
    ]
  }
  